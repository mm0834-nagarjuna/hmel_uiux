<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Predefined - Key Performance Area"
		title="Predefined - Key Performance Area"
		confirm=".onClickCancel"
		cancel=".onClickCancel"
		class="mvnTableSelectDialog"
		items="{
			path : 'PreDefineGoal>/indv_goals'
		}">
		<ColumnListItem vAlign="Top">
			<cells>
				<Text text="{PreDefineGoal>KRA}" />
				<Text text="{PreDefineGoal>KPI}" />
				<Text text="{PreDefineGoal>Target}" />
				<Text text="{PreDefineGoal>Weightage}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="Key Strategic Priority - Key Results Area" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Key Performance Index" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Target" />
				</header>
			</Column>
			<Column width="10rem">
				<header>
					<Text text="Weightage(%)" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>