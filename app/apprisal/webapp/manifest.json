{
  "_version": "1.65.0",
  "sap.app": {
    "id": "apprisal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.4",
      "toolsId": "52f3fb98-e9ee-4f8b-ae5f-8275b3c428fc"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
        
      },
      "PreDefineGoal":{
          "uri": "model/pregoals.json",
          "type": "JSON"
        }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "apprisal.i18n.i18n"
        }
      },
      "apprisalData": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "PreDefineGoal": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "PreDefineGoal"
      },
      "CustomData":{
        "type": "sap.ui.model.json.JSONModel"

      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "apprisal.view",
        "async": true,
        "viewPath": "apprisal.view"
      },
      "routes": [
        {
          "name": "RouteList",
          "pattern": ":?query:",
          "target": [
            "TargetList"
          ]
        },
        {
          "name": "apprisalDetail",
          "pattern": "index={index}&appID={appId}",
          "target": [
            "TargetApprisalDetail"
          ]
        }
      ],
      "targets": {
        "TargetList": {
          "id": "List",
          "name": "List"
        },
        "TargetApprisalDetail": {
          "id": "Detail",
          "name": "Detail"
        }
      }
    },
    "rootView": {
      "viewName": "apprisal.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}